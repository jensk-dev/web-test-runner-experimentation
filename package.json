{
  "name": "packageName",
  "version": "0.0.0",
  "description": "",
  "repository": "unjs/packageName",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "debug": "unbuild && node --inspect-brk dist/index.cjs",
    "start": "unbuild && node dist/index.cjs",
    "build": "unbuild",
    "dev": "vitest dev",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.6",
    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.51.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.20.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.6.12",
  "dependencies": {
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@open-wc/testing": "^3.2.0",
    "@web/dev-server": "^0.3.3",
    "@web/dev-server-core": "^0.5.2",
    "@web/dev-server-esbuild": "^0.4.1",
    "@web/dev-server-rollup": "^0.5.2",
    "@web/test-runner": "^0.17.2",
    "@web/test-runner-commands": "^0.8.1",
    "@web/test-runner-core": "^0.11.4",
    "@web/test-runner-mocha": "^0.8.1",
    "@web/test-runner-puppeteer": "^0.14.0",
    "mocha": "^10.2.0",
    "puppeteer": "^21.3.8"
  }
}
